{
    "name": "@alto/cli",
    "version": "0.0.1",
    "description": "A performant and modular ERC-4337 Bundler written in Typescript",
    "repository": "https://github.com/pimlicolabs/alto.git",
    "author": "Pimlico",
    "license": "GPL-3.0-or-later",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "files": [
        "lib/**/*.d.ts",
        "lib/**/*.js",
        "lib/**/*.js.map",
        "*.d.ts",
        "*.js"
    ],
    "scripts": {
        "build": "tsc -p ./tsconfig.build.json",
        "test": "test -d test && mocha test/**/*.test.ts --exit || echo 'No test folder found. Skipping tests.'",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix"
    },
    "dependencies": {
        "@alto/executor": "*",
        "@alto/mempool": "*",
        "@alto/rpc": "*",
        "@alto/types": "*",
        "@alto/utils": "*",
        "@opentelemetry/api": "^1.7.0",
        "@opentelemetry/auto-instrumentations-node": "^0.40.2",
        "@opentelemetry/core": "^1.19.0",
        "@opentelemetry/exporter-trace-otlp-proto": "^0.46.0",
        "@opentelemetry/sdk-node": "^0.46.0",
        "@sentry/node": "^7.83.0",
        "@types/node": "^18.16.3",
        "dotenv": "^16.0.3",
        "opentelemetry-instrumentation-fetch-node": "^1.1.2",
        "prom-client": "^14.2.0",
        "viem": "^1.19.1",
        "yargs": "^17.7.1",
        "zod": "^3.21.4",
        "zod-validation-error": "^1.3.0"
    },
    "devDependencies": {
        "@types/yargs": "^17.0.24"
    }
